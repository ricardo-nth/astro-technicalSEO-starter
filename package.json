{
  "name": "boilerplate-vps",
  "type": "module",
  "version": "0.0.1",
  "packageManager": "pnpm@9.15.0",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "check": "astro check",
    "astro": "astro",
    "analyze": "node scripts/analyze-bundle.js",
    "audit": "npm audit && astro check",
    "lighthouse": "lighthouse http://localhost:4321 --view",
    "test:accessibility": "axe http://localhost:4321",
    "optimize:images": "node scripts/optimize-images.js",
    "clean": "rm -rf dist .astro node_modules/.cache",
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.ts,.astro",
    "precommit": "npm run check && npm run build"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/partytown": "^2.1.4",
    "@astrojs/sitemap": "^3.5.1",
    "@astrojs/vercel": "^8.2.6",
    "@sentry/browser": "^10.8.0",
    "@sentry/integrations": "^7.114.0",
    "astro": "^5.13.4",
    "astro-compressor": "^1.0.0",
    "astro-robots-txt": "^1.0.0",
    "sharp": "^0.34.3",
    "typescript": "^5.9.2",
    "web-vitals": "^5.1.0"
  },
  "devDependencies": {
    "terser": "^5.43.1"
  }
}